meta {
  name: Assign role to user
  type: http
  seq: 1
}

post {
  url: {{BASE_URL}}/api/v1/user-roles/{{USER_ID}}/roles/{{ROLE_ID}}
  body: json
  auth: none
}

headers {
  x-api-key: {{API_KEY}}
  x-api-secret: {{API_SECRET}}
  content-type: application/json
  Authorization: Bearer {{USER_ACCESS_TOKEN}}
}
